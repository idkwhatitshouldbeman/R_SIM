FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    
    outlet
    {
        type            zeroGradient;
    }
    
    rocketBody
    {
        type            noSlip;
    }
    
    // Moving fin boundaries - updated by control system
    fin1
    {
        type            noSlip;
    }
    
    fin2
    {
        type            noSlip;
    }
    
    fin3
    {
        type            noSlip;
    }
    
    fin4
    {
        type            noSlip;
    }
    
    symmetry
    {
        type            symmetry;
    }
    
    frontAndBack
    {
        type            empty;
    }
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
